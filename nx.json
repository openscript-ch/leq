{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": ["{workspaceRoot}/dist/{projectName}"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  },
  "useDaemonProcess": false
}
